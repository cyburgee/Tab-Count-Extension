{
  "name": "Tabbism",
  "description": "Record the Lives of Your Tabs",
  "version": "0.5",
  "permissions": [
    "http://*/*", 
    "unlimitedStorage",
    "storage",
    "tabs",
    "alarms"
    ],
  "background": {
    "persistent": true,
    "scripts": ["background.js", "d3.v3.min.js"]
  },
  "browser_action": {
    "default_title": "Show Tab Visualization"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2
}
